{
   "env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"airbnb-base",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"

	},
	"rules": {
		"linebreak-style": 0,
		"indent": [2, "tab"],
        "no-tabs": 0,
		"no-param-reassign": 0,
		"import/extensions": 0
	}
}
